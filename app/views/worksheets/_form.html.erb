<%= simple_form_for(@worksheet) do |f| %>
  <%= f.error_notification %>


     <div class="form-inputs">
    <%= f.input :interest, input_html: { class: 'form-control'} %>
  	 </div>
     <div class="form-inputs">
    <%= f.input :rent, input_html: { class: 'form-control'} %>
  	 </div>
     <div class="form-inputs">
    <%= f.input :account_fees, input_html: { class: 'form-control'} %>
  	 </div>

  <div class="form-actions">
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  </div>
<% end %>
